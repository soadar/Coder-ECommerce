<main class="form-signin w-25 m-auto mt-3">
  <form action="/register" method="post">
    <h1 class="h3 mb-3 fw-normal text-center">Inicio de sesion</h1>

    <div class="form-floating mt-3">
      <input
        type="text"
        class="form-control"
        id="first_name"
        name="first_name"
        placeholder=""
        required
      />
      <label for="first_name">Nombre</label>
    </div>

    <div class="form-floating mt-3">
      <input
        type="text"
        class="form-control"
        id="last_name"
        name="last_name"
        placeholder=""
        required
      />
      <label for="last_name">Apellido</label>
    </div>

    <div class="form-floating mt-3">
      <input
        type="email"
        class="form-control"
        id="email"
        name="email"
        placeholder=""
        required
      />
      <label for="email">Email</label>
    </div>

    <div class="form-floating mt-3">
      <input
        type="number"
        class="form-control"
        id="age"
        name="age"
        placeholder=""
        required
      />
      <label for="age">Edad</label>
    </div>

    <div class="form-floating my-3">
      <input
        type="password"
        class="form-control"
        id="password"
        name="password"
        placeholder=""
        required
      />
      <label for="password">Contrase√±a</label>
    </div>
    <button
      class="btn btn-primary w-100 my-2"
      type="submit"
    >Registrarme</button>
  </form>
  <a class="btn btn-primary w-100 my-2" href="/login">Iniciar Sesion</a>
</main>